{
  "name": "project",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "ts-node-dev src/shared/infra/http/server.ts",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "dev:nodemon": "nodemon --legacy-watch src/server.js",
    "dev:server": "ts-node-dev -r tsconfig-paths/register --inspect=0.0.0.0:9333 --poll --respawn --transpile-only --ignore-watch node_modules src/shared/infra/http/server.ts",
    "seed:generate": "ts-node-dev ./node_modules/typeorm/cli.js migration:generate -c seed -n ",
    "seed:run": "ts-node-dev ./node_modules/typeorm/cli.js migration:run -c seed",
    "seed:revert": "ts-node-dev ./node_modules/typeorm/cli.js migration:revert -c seed"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "date-fns": "^2.16.1",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "faker": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.4",
    "tsyringe": "^4.3.0",
    "typeorm": "^0.2.26",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.8",
    "@types/faker": "^5.1.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.4",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1",
    "ts-node-dev": "^1.0.0-pre.62",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.2"
  }
}
